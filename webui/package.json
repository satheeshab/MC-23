{
  "name": "webui",
  "private": true,
  "version": "1.0.0",
  "description": "webui - server side (runs on hosting machine)",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "pkginstall": "node customPackageinstall.js",
    "prepare": "patch-package",
    "start": "node createEnv.js && pm2 start config/ecosystem.prod.config.js --no-daemon",
    "start:dev": "concurrently -k \"npm run start:dev:server\" \"npm run start:dev:client\"",
    "start:dev:server": "npm run lint && pm2 start config/ecosystem.dev.config.js --no-daemon",
    "start:dev:client": "cd client && npm run start:dev",
    "start:test": "concurrently -k -s first \"npm run start:test:server\" \"npm run start:test:client\"",
    "start:test:server": "npm run lint && pm2 start config/ecosystem.test.config.js --no-daemon",
    "start:test:client": "cd client && npm run start:test",
    "start:build": "npm-run-all build:client build:server",
    "start:build:client": "cd client && npm run build",
    "start:build:server": "npm run lint && npm run clean && cross-env NODE_ENV=production babel server -d dist -s --ignore 'server/**/*.spec.js', 'server/**/*.test.js'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --ext js",
    "stop": "pm2 stop all"
  },
  "license": "",
  "dependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "fetch-retry": "^5.0.6",
    "http-errors": "^2.0.0",
    "pm2": "^5.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/eslint-parser": "^7.21.8",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.21.0",
    "@babel/runtime": "^7.21.5",
    "browser-resolve": "^2.0.0",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "cypress": "^12.12.0",
    "cypress-dotenv": "^2.0.0",
    "enzyme": "^3.11.0",
    "eslint": "^8.41.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "patch-package": "^7.0.0"
  }
}
